programa {
  funcao inicio() {
    inteiro num1,num2, soma, sub, div, mult

    escreva("Digite o primeiro número: ")
    leia(num1)
    escreva("Digite o segundo número: ")
    leia(num2)
    soma=num1+num2
    sub=num1-num2
    div=num1/num2
    mult=num1*num2
    escreva("A soma dos dois números é ", soma, ", a subtração é ", sub, ", a divisão é ", div,", a multiplicação é ", mult)
  }
}
